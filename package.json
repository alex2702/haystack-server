{
  "private": true,
  "name": "haystack-server",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "lib/index.js",
  "scripts": {
    "create-db": "psql -U haystack_user < src/database/schema.sql",
    "seed": "npm run create-db && ts-node src/database/seed.ts",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "loadtest": "colyseus-loadtest loadtest/example.ts --room haystack_room --numClients 2",
    "build": "npm run clean && tsc && node node_modules/copyfiles/copyfiles package.json ./lib && node node_modules/copyfiles/copyfiles arena.env ./lib && node node_modules/copyfiles/copyfiles ./data/cities.json ./lib",
    "clean": "node node_modules/rimraf/bin lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.14.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "copyfiles": "^2.4.1",
    "rimraf": "^2.7.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@colyseus/command": "^0.2.0",
    "@colyseus/monitor": "^0.14.22",
    "colyseus": "^0.14.23",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "geolib": "^3.3.3",
    "pg": "^8.7.1"
  }
}
